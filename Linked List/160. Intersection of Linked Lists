/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *getIntersectionNode(ListNode *headA, ListNode *headB) {
        ListNode* cur = headB;
        while(headA != NULL)
        {
            while(headB != NULL)
            {
                if(headA == headB)
                {
                    return headA;
                }
                headB = headB -> next;
            }
            headB = cur;
            headA = headA -> next;
        }
        return headA;
    }
};
